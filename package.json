{
  "name": "electron-http-listener",
  "version": "1.0.0",
  "description": "A simple Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build_win32": "electron-builder --win --ia32"
  },
  "build": {
    "directories": {
      "output": "MapleNotifier"
    },
    "productName": "MapleNotifier",
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    }
  },

  "repository": "https://github.com/spff/electronHttpListener",
  "keywords": [
    "Electron",
    "http"
  ],
  "author": "Inspect Workshop",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-packager": "^10.1.0",
    "electron-builder": "^19.47.1"
  },
  "dependencies": {
    "socket.io-client": "^2.0.4"
  }
}