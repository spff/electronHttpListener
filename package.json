{
  "name": "electron-http-listener",
  "version": "1.0.0",
  "description": "A simple Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build_mac": "electron-packager . hello --overwrite --platform=darwin --arch=x64 --ignore=node_modules/electron-* ",
    "build_win32": "electron-packager . hello --overwrite --platform=win32 --arch=x64 --ignore=node_modules/electron-* ",
    "build_linux": "electron-packager . hello --overwrite --platform=linux --arch=x64 --ignore=node_modules/electron-* "
  },
  "repository": "https://github.com/spff/electronHttpListener",
  "keywords": [
    "Electron",
    "http"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-packager": "^10.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2"
  }
}
